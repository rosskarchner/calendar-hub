{
  "Version": "2012-10-17",
  "Statement": [
    {
      "Effect": "Allow",
      "Action": [
        "sns:ConfirmSubscription",
        "sns:Receive"
      ],
      "Resource": "arn:aws:sns:*:*:newsletter-feedback"
    },
    {
  "Effect": "Allow",
  "Action": [
    "kms:GenerateMac",
    "kms:VerifyMac"
  ],
  "Resource": "arn:aws:kms:region:account-id:key/your-key-id"
    },
    {
      "Effect": "Allow",
      "Action": [
        "logs:CreateLogGroup",
        "logs:CreateLogStream",
        "logs:PutLogEvents"
      ],
      "Resource": "arn:aws:logs:*:*:*"
    },
    {
      "Effect": "Allow",
      "Action": [
        "dynamodb:PutItem",
        "dynamodb:GetItem",
        "dynamodb:UpdateItem",
        "dynamodb:DeleteItem",
        "dynamodb:Scan",
        "dynamodb:Query"
      ],
      "Resource": "arn:aws:dynamodb:*:*:table/newsletter_subscribers"
    },
    {
            "Effect": "Allow",
            "Action": [
                "ses:CreateContact",
                "ses:DeleteContact",
                "ses:ListContacts",
                "ses:SendEmail",
                "ses:UpdateContact"
            ],
            "Resource": "*"
        },
    {
      "Effect": "Allow",
      "Action": [
        "secretsmanager:GetSecretValue"
      ],
      "Resource": "arn:aws:secretsmanager:*:*:secret:newsletter/csrf_secret-*"
    }
  ]
}